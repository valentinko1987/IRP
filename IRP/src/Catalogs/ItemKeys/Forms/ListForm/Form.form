		</edit>
		<main>true</main>
		<extInfo xsi:type="form:DynamicListExtInfo">
			<queryText>SELECT
	ItemKeys.Ref,
	ItemKeys.DeletionMark,
	ItemKeys.Item,
	ItemKeys.Unit,
	ItemKeys.Specification,
	ItemKeys.Predefined,
	ItemKeys.PredefinedDataName,
	CASE
		WHEN ItemKeys.DeletionMark
		AND ItemKeys.Specification = VALUE(Catalog.Specifications.EmptyRef)
			THEN 5
		WHEN ItemKeys.DeletionMark
		AND ItemKeys.Specification.Type = VALUE(Enum.SpecificationType.Set)
			THEN 4
		WHEN ItemKeys.DeletionMark
		AND ItemKeys.Specification.type = VALUE(Enum.SpecificationType.Bundle)
			THEN 6
		WHEN ItemKeys.Specification = VALUE(Catalog.Specifications.EmptyRef)
			THEN 1
		WHEN ItemKeys.Specification.Type = VALUE(Enum.SpecificationType.Set)
			THEN 0
		WHEN ItemKeys.Specification.type = VALUE(Enum.SpecificationType.Bundle)
			THEN 2
	END AS IsSpecification
FROM
	Catalog.ItemKeys AS ItemKeys</queryText>
			<mainTable>Catalog.ItemKeys</mainTable>
			<dynamicDataRead>true</dynamicDataRead>
			<autoFillAvailableFields>true</autoFillAvailableFields>
			<customQuery>true</customQuery>
			<autoSaveUserSettings>true</autoSaveUserSettings>
			<getInvisibleFieldPresentations>true</getInvisibleFieldPresentations>
		</extInfo>
